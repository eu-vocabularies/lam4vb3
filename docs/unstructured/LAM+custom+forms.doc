    <h1>LAM custom forms</h1>
    <div class=3D"Section1">
        <p><br></p>
<h1 id=3D"LAMcustomforms-Customformforpropertyshape">Custom form for proper=
ty shape</h1>
<p><br></p>
<div class=3D"code panel pdl" style=3D"border-width: 1px;">
<div class=3D"codeContent panelContent pdl">=20
<pre class=3D"syntaxhighlighter-pre" data-syntaxhighlighter-params=3D"brush=
: sql; gutter: true; theme: Confluence" data-theme=3D"Confluence">prefix  x=
sd:    &lt;http://www.w3.org/2001/XMLSchema#&gt;
prefix  rdf:    &lt;http://www.w3.org/1999/02/22-rdf-syntax-ns#&gt;
prefix  dct:    &lt;http://purl.org/dc/terms/&gt;
prefix  coda:   &lt;http://art.uniroma2.it/coda/contracts/&gt;
prefix skosxl:  &lt;http://www.w3.org/2008/05/skos-xl#&gt;
prefix  codaImpl:   &lt;http://art.uniroma2.it/coda/converters/&gt;
prefix euvoc: &lt;http://publications.europa.eu/ontology/euvoc#&gt;
prefix sh: &lt;http://www.w3.org/ns/shacl#&gt;=20
prefix cdm: &lt;http://publications.europa.eu/ontology/cdm#&gt;
prefix lam: &lt;http://publications.europa.eu/resources/authority/lam/&gt;

rule it.uniroma2.art.semanticturkey.customform id:lamShape {
    nodes =3D {
        shapeId uri(coda:randIdGen("sh", {})) .
=09=09path uri userPrompt/path_uri .
        lang literal userPrompt/lang .
        nameLiteral literal(coda:langString($lang)) userPrompt/name .
=09=09descriptionLiteral literal(coda:langString($lang)) userPrompt/descrip=
tion .
=09=09minCar literal^^xsd:integer userPrompt/mininum_cardinality .
=09=09maxCar literal^^xsd:integer userPrompt/maximum_cardinality .=09=09
=09=09rangeClass uri userPrompt/range_class .
=09=09rangeDatatype uri userPrompt/range_data_type .
=09=09rangeValue literal userPrompt/has_value .
=09=09annotatedWithProperty uri userPrompt/annotated_with_property .
=09=09annotationPropertyRange uri userPrompt/range_of_annotation_property .
    }
    graph =3D {
        $shapeId a sh:PropertyShape .
=09=09$shapeId sh:path $path .
=09=09$shapeId sh:name $nameLiteral .
=09=09
=09=09OPTIONAL{
=09=09        $shapeId sh:description $descriptionLiteral  .=09=09=09=09
=09=09=09    $shapeId sh:minCount $minCar .
=09=09=09=09$shapeId sh:maxCount $maxCar .

=09=09=09=09$shapeId sh:hasValue $rangeClass .
=09=09=09=09$shapeId sh:hasValue $rangeDatatype .
=09=09=09=09$shapeId sh:hasValue $rangeValue .
=09=09=09=09$shapeId sh:hasValue $annotatedWithProperty .
=09=09=09=09$shapeId sh:hasValue $annotationPropertyRange .
=09=09}=09=09
       =20
    }
}</pre>=20
